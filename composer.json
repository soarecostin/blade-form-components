{
    "name": "soarecostin/blade-form-components",
    "description": "Laravel Blade form components",
    "keywords": [
        "soarecostin",
        "blade-form-components"
    ],
    "homepage": "https://github.com/soarecostin/blade-form-components",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Costin Soare",
            "email": "soarecostin@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "laravel/framework": "5.6.* || 5.7.* || 5.8.* || 6.*.*",
        "illuminate/support": "5.6.* || 5.7.* || 5.8.* || 6.*.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.6.*|3.7.*|3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "SoareCostin\\BladeFormComponents\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SoareCostin\\BladeFormComponents\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SoareCostin\\BladeFormComponents\\BladeFormComponentsServiceProvider"
            ]
        }
    }
}
